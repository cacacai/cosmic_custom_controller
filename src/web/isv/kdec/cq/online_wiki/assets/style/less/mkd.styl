.mkd_area {
  .markdown-body {
    .shadow {
      -box-shadow 0px 0x 0x
    }
    .v-note-op {
      .fa-mavon-picture-o {
        .op-image {
          .dropdown-images {
            display none
          }
        }
      }
    }
    .v-note-panel {
      border none
      height 500px
      overflow-y auto
      flex none
      flex-grow 1
      .single-show {
        .v-show-content {
          background transparent !important
          padding 0
        }
      }
    }
  }
  .markdown-body {
    -ms-text-size-adjust 100%
    -webkit-text-size-adjust 100%
    color rgba(51, 51, 51, 1)
    font-family -apple-system, system-ui, BlinkMacSystemFont, Segoe UI, Helvetica, Arial, sans-serif
    font-size 14px
    font-weight 400
    line-height 26px
    word-wrap break-word
    background-color #fff
    width 100%
    margin 30px auto 20px
    margin 0 auto 0
    border none
    pre {
      display block
      padding 9.5px
      margin 10px 0 10px 0
      line-height 1.42857143
      color #2B87F3
      word-break break-all
      word-wrap break-word
      background #E4EFFF
      border-radius 3px
      line-height 24px
      font 14px SFMono-Regular, Consolas, Liberation Mono, Menlo, Courier, monospace
    }
    code {
      padding 2px 6px
      color #2B87F3
      background #E4EFFF
      border-radius 3px
      line-height 24px
      font 14px SFMono-Regular, Consolas, Liberation Mono, Menlo, Courier, monospace
    }
    pre code {
      padding 0
      font-size inherit
      color #2B87F3
      white-space pre-wrap
      background-color transparent
      border-radius 0
    }
    pre code span {
      line-height 24px
      font-family PingFangSC-Regular
    }
    .hljs {
      display block
      overflow-x auto
      padding 0.5em
      background #e4efff
      color black
    }
    .hljs-link, .hljs-emphasis, .hljs-attribute, .hljs-addition {
      color #070
    }
    .hljs-emphasis {
      font-style italic
    }
    .hljs-strong, .hljs-string, .hljs-deletion {
      color #d14
    }
    .hljs-strong {
      font-weight bold
    }
    .hljs-quote {
      color #998
      font-style italic
    }
    .hljs-comment {
      font-size 14px
      color #998
    }
    .hljs-section, .hljs-title {
      color #900
    }
    .hljs-class .hljs-title, .hljs-type {
      color #458
    }
    .hljs-variable, .hljs-template-variable {
      color #336699
    }
    .hljs-bullet {
      color #997700
    }
    .hljs-meta {
      color #3344bb
    }
    .hljs-code, .hljs-number, .hljs-literal, .hljs-keyword, .hljs-selector-tag {
      color #099
    }
    .hljs-regexp {
      background-color #fff0ff
      color #880088
    }
    .hljs-symbol {
      color #990073
    }
    .hljs-tag, .hljs-name, .hljs-selector-id, .hljs-selector-class {
      color #007700
    }
    p {
      line-height 26px
      margin-bottom 10px
    }
    p + ul, p + ol {
      margin-top -10px
    }
    li p {
      margin-bottom 0px
    }
    blockquote p {
      margin-bottom 10px
    }
    .pl-c {
      color #969896
    }
    .pl-c1, .pl-s .pl-v {
      color #0086b3
    }
    .pl-e, .pl-en {
      color #795da3
    }
    .pl-s .pl-s1, .pl-smi {
      color #333
    }
    .pl-ent {
      color #63a35c
    }
    .pl-k {
      color #a71d5d
    }
    .pl-pds, .pl-s, .pl-s .pl-pse .pl-s1, .pl-sr, .pl-sr .pl-cce, .pl-sr .pl-sra, .pl-sr .pl-sre {
      color #183691
    }
    .pl-smw, .pl-v {
      color #ed6a43
    }
    .pl-bu {
      color #b52a1d
    }
    .pl-c2, .pl-ii {
      color #f8f8f8
      background-color #b52a1d
    }
    .pl-c2:before {
      content '^M'
    }
    .pl-sr .pl-cce {
      font-weight 700
      color #63a35c
    }
    .pl-ml {
      color #693a17
    }
    .pl-mh, .pl-mh .pl-en, .pl-ms {
      font-weight 700
      color #1d3e81
    }
    .pl-mq {
      color teal
    }
    .pl-mi {
      font-style italic
      color #333
    }
    .pl-mb {
      font-weight 700
      color #333
    }
    .pl-md {
      color #bd2c00
      background-color #ffecec
    }
    .pl-mi1 {
      color #55a532
      background-color #eaffea
    }
    .pl-mc {
      color #ef9700
      background-color #ffe3b4
    }
    .pl-mi2 {
      color #d8d8d8
      background-color gray
    }
    .pl-mdr {
      font-weight 700
      color #795da3
    }
    .pl-mo {
      color #1d3e81
    }
    .pl-ba {
      color #595e62
    }
    .pl-sg {
      color silver
    }
    .pl-corl {
      text-decoration underline
      color #183691
    }
    .octicon {
      display inline-block
      vertical-align text-top
      fill currentColor
    }
    a {
      background-color transparent
      -webkit-text-decoration-skip objects
    }
    a:active, a:hover {
      outline-width 0
    }
    img {
      display block
      border-style none
      background-color hsla(0, 0%, 100%, 0.1)
      max-width 100%
      box-sizing content-box
      vertical-align middle
      margin 10px 0
      border 1px solid #ccc
      cursor zoom-in
    }
    svg:not(:root) {
      overflow hidden
    }
    hr {
      box-sizing content-box
      overflow visible
    }
    input {
      font inherit
      margin 0
      overflow visible
    }
    [type=checkbox] {
      box-sizing border-box
      padding 0
    }
    * {
      box-sizing border-box
    }
    input {
      font-family inherit
      font-size inherit
      line-height inherit
    }
    a {
      color #0366d6
      text-decoration none
    }
    a:hover {
      text-decoration underline
    }
    strong {
      font-weight bold
    }
    hr {
      height 0
      margin 15px 0
      overflow hidden
      background transparent
      border-bottom 1px solid #dfe2e5
    }
    hr:after, hr:before {
      display table
      content ''
    }
    hr:after {
      clear both
    }
    h1, h2, h3, h4, h5, h6 {
      color #333333
      margin-top 30px
      margin-bottom 10px
      font-family PingFangSC-Regular
      letter-spacing 0
      font-weight bold
      font-weight 400
      border-bottom none
    }
    h1 {
      font-size 30px
      margin-bottom 5px
      line-height 42px
    }
    h2 {
      font-size 26px
    }
    h3 {
      font-size 24px
      line-height 32px
    }
    h4 {
      font-size 20px
      line-height 22px
    }
    h5 {
      font-size 18px
    }
    h6 {
      font-size 16px
    }
    ol, ul {
      margin-top 10px
      margin-bottom 6px
      padding-left 1.5em
    }
    ol ol, ul ol {
      list-style-type lower-roman
      list-style-type disc
    }
    ol ol ol, ol ul ol, ul ol ol, ul ul ol {
      list-style-type lower-alpha
      list-style-type disc
    }
    dd {
      margin-left 0
    }
    .octicon {
      vertical-align text-bottom
    }
    .pl-0 {
      padding-left 0 !important
    }
    .pl-1 {
      padding-left 4px !important
    }
    .pl-2 {
      padding-left 8px !important
    }
    .pl-3 {
      padding-left 16px !important
    }
    .pl-4 {
      padding-left 24px !important
    }
    .pl-5 {
      padding-left 32px !important
    }
    .pl-6 {
      padding-left 40px !important
    }
    &:after, &:before {
      display table
      content ''
    }
    &:after {
      clear both
    }
    .v-show-content>:first-child {
      margin-top 0 !important
    }
    .v-show-content>:last-child {
      margin-bottom 0 !important
    }
    a:not([href]) {
      color inherit
      text-decoration none
    }
    .anchor {
      float left
      padding-right 4px
      margin-left -20px
      line-height 1
    }
    .anchor:focus {
      outline none
    }
    hr {
      height 0.25em
      padding 0
      margin 24px 0
      background-color #e1e4e8
      border 0
    }
    blockquote {
      padding 0 1em
      margin 0
      color #6a737d
      border-left 0.25em solid #dfe2e5
    }
    blockquote>:first-child {
      -margin-top 0
    }
    blockquote>:last-child {
      -margin-bottom 0
    }
    kbd {
      font-size 11px
      background-color #fafbfc
    }
    h1 .octicon-link, h2 .octicon-link, h3 .octicon-link, h4 .octicon-link, h5 .octicon-link, h6 .octicon-link {
      color #1b1f23
      vertical-align middle
      visibility hidden
    }
    h1:hover .anchor, h2:hover .anchor, h3:hover .anchor, h4:hover .anchor, h5:hover .anchor, h6:hover .anchor {
      text-decoration none
    }
    h1:hover .anchor .octicon-link, h2:hover .anchor .octicon-link, h3:hover .anchor .octicon-link, h4:hover .anchor .octicon-link, h5:hover .anchor .octicon-link, h6:hover .anchor .octicon-link {
      visibility visible
    }
    ol ol, ol ul, ul ol, ul ul {
      margin-top 0
      margin-bottom 0
    }
    dl {
      padding 0
    }
    dl dt {
      padding 0
      margin-top 10px
      font-size 1em
      font-style italic
      font-weight 600
    }
    dl dd {
      padding 0 10px
      margin-bottom 10px
    }
    table {
      display block
      width 100%
      overflow auto
      display table
      border-spacing 0
      border-collapse collapse
      margin 10px auto
    }
    table td, table th {
      padding 10px
      text-align left
      color #666666
      line-height 24px
      border 1px solid rgba(221, 221, 221, 1)
      vertical-align middle
    }
    table th {
      font-weight bold
      color #333
      background #f4f6fa
    }
    table tr {
      background-color #fff
      border-top 1px solid #c6cbd1
    }
    kbd {
      display inline-block
      padding 3px 5px
      font 11px SFMono-Regular, Consolas, Liberation Mono, Menlo, Courier, monospace
      line-height 10px
      color #444d56
      vertical-align middle
      background-color #fcfcfc
      border 1px solid #c6cbd1
      border-bottom-color #959da5
      border-radius 3px
      box-shadow inset 0 -1px 0 #959da5
    }
    :checked+.radio-label {
      position relative
      z-index 1
      border-color #0366d6
    }
    .task-list-item {
      list-style-type none
    }
    .task-list-item+.task-list-item {
      margin-top 3px
    }
    .task-list-item input {
      margin 0 0.2em 0.25em -1.6em
      vertical-align middle
    }
    hr {
      border-bottom-color #eee
    }
    ul {
      list-style-type disc
    }
    ul>li>ul {
      list-style-type circle
      list-style-type disc
    }
    ul>li>ul>li>ul {
      list-style-type square
      list-style-type disc
    }
    ul>li>ul>li>ul>li>ul {
      list-style-type lower-roman
      list-style-type disc
    }
    ol {
      list-style-type decimal
      list-style-type disc
    }
    hr {
      height 2px
      padding 0
      margin 24px 0
      background-color #eaecef
      border 0
    }
    em, i {
      font-style italic
    }
    /* # sourceMappingURL=mditor.min.css.map */
    .item {
      cursor pointer
    }
    .content {
      display flex
      flex 1
    }
    .root {
      display flex
      width 100%
      height 100%
      flex-direction column
    }
    .tree {
      display flex
      width 300px
      flex-shrink 0
      flex-direction column
      padding-top 20px
    }
    .treeUL {
      list-style none
